---
layout: base.njk
pagination:
  data: collections
  size: 1
  alias: tag
  addAllPagesToCollections: true 
  filter:
    - all

permalink: /tagged/{{ tag }}/

eleventyComputed:
  title: Tagged “{{ tag }}”

addAllPagesToCollections: true 

---
<h1>Tagged “{{tag}}”</h1>

<ol>
  {% for tagPage in collections[tag] %}
    <li>
      <a href="{{ tagPage.url }}">{{ tagPage.data.title or tagPage.filePathStem }}</a>
    </li>
  {% endfor %}
</ol>

<p>
  <a href="/tags">Back to tag directory</a>
</p>